# LDAP 연결 설정
LDAP_URL=ldap://dsm-server:389
LDAP_BASE_DN=dc=dsm,dc=local
LDAP_BIND_DN=cn=admin,dc=dsm,dc=local
LDAP_BIND_PASSWORD=your-ldap-admin-password

# Redis 설정
REDIS_PASSWORD=redis-password-change-this-in-production

# JWT 설정
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# 데이터베이스 설정
DB_PASSWORD=your-postgres-password-change-this-in-production

# OIDC 설정
OIDC_ISSUER=http://localhost:3002
OIDC_CLIENT_ID=your-oidc-client-id
OIDC_CLIENT_SECRET=your-oidc-client-secret

# SAML 설정 (주석 처리됨 - 필요시 활성화)
# SAML_ENTITY_ID=http://localhost:3002/saml
# SAML_SSO_URL=http://localhost:3002/saml/sso

# CORS 설정
CORS_ORIGIN=http://localhost

# 로그 레벨
LOG_LEVEL=info
